		uniform sampler2D _MainTex;
		uniform float4 _MainTex_ST;
		uniform sampler2D _SubTex;
		uniform float4 _SubTex_ST;
		uniform float _DisturbStr;

		inline half4 LightingStandardCustomLighting( inout SurfaceOutputCustomLightingCustom s, half3 viewDir, UnityGI gi )
		{
			UnityGIInput data = s.GIData;
			Input i = s.SurfInput;
			half4 c = 0;
			float2 uv_MainTex = i.uv_texcoord * _MainTex_ST.xy + _MainTex_ST.zw;
			float2 temp_cast_0 = (0.5).xx;
			float2 uv_SubTex = i.uv_texcoord * _SubTex_ST.xy + _SubTex_ST.zw;
			c.rgb = 0;
			c.a = tex2D( _MainTex, ( uv_MainTex + ( ( uv_MainTex - temp_cast_0 ) * tex2D( _SubTex, uv_SubTex ).r * _DisturbStr ) ) ).r;
